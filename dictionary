meme_dict = {
    "кринж": "Что-то очень странное или стыдное",
    "лол": "Что-то очень смешное",
    "рофл": "Шутка",
    "криповый": "Страшный, пугающий",
    "агриться": "Злиться",
    "имба": "Что-то крутое или несбалансированное(от слова imbalance)",
    "мем": "Шутка или прикол в виде картинки",
    "пруфы": "Доказательства",
    "ф": "ф",
            }
a = int(input("Введите сколько раз нужно использовать программу: "))
for i in range (a):
    word = input("Введите слово: ")
    word=word.lower()
    if word in meme_dict.keys():
        print("Перевод: ", meme_dict[word])
    else:
        print("Данного слова нет")
